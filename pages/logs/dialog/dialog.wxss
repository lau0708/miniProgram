/* pages/logs/dialog.wxss */
.shopcartyin{
  width:100%;
  height:100%;
}
.cart{
  width:100%;
  height:50%;
  background: white;
  position: absolute;
  bottom:0px;
}
.cart-yi{
  display: flex;
  justify-content: space-between;
  padding:36rpx;
  background: #f3f5f7;
}
.shopcart{
  font-size:28rpx;
}
.empty{
  font-size:24rpx;
  color: rgb(0,160,220);
}
/* 列表 */
.list{
  padding: 0rpx 36rpx;
}
.list-yi{
  height:96rpx;
  padding: 24rpx 0rpx;
  border-bottom:1rpx solid rgba(7,17,27,0.4);
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
}
.list-yi .one{
  font-size:28rpx;
}
.two-yi{
  margin-right: 24rpx;
  color:red;
  font-size:28rpx;
}
.two{
  width:240rpx;
  display: flex;
}
.two-er{
  display: flex;
}
.two-er .jian,.two-er .jia{
  width:50rpx;
  height:50rpx;
  border:1px solid #00a0dc;
  color:#00a0dc;
  border-radius:50px;
  font-size:60rpx;
  text-align: center;
  line-height: 45rpx;
}
.two-er .num{
  font-size:30rpx;
  line-height: 50rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;

}
.yin{
  width:100%;
  height:50%;
}